<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace = "Catalogo">
    <select id = "getAllCatalogos" resultType = "pojos.Catalogo">
        SELECT
        catalogo.idCatalogo,
        catalogo.idTipo,
        catalogo.nombre,
        catalogo.orden
        FROM catalogo
    </select>
</mapper>

